{
  "name": "buildstamp-bin",
  "version": "1.1.2",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Buildstamp binaries for MacOS, Linux and Windows",
  "scripts": {
    "clean": "rimraf target",
    "build:bins": "pkg --out-path=./target/bin --targets=node14-linux-x64,node14-macos-x64,node14-win-x64 ../core",
    "build": "yarn clean && yarn build:bins"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi/buildstamp.git"
  },
  "keywords": [
    "build-info",
    "buildstamp"
  ],
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target"
  ],
  "license": "MIT",
  "devDependencies": {
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "buildstamp": "1.4.2"
  }
}
