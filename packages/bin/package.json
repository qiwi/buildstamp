{
  "name": "buildstamp-bin",
  "version": "1.2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bin": "./src/main/js/index.js",
  "description": "Buildstamp binaries for MacOS, Linux and Windows",
  "scripts": {
    "clean": "rimraf target",
    "build:bins": "caxa --input ./ --output 'target/bin/buildstamp' -- '{{caxa}}/node_modules/.bin/node' '{{caxa}}/src/main/js/index.js'",
    "build": "yarn clean && yarn build:bins"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi/buildstamp.git"
  },
  "keywords": [
    "build-info",
    "buildstamp"
  ],
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target"
  ],
  "license": "MIT",
  "devDependencies": {
    "caxa": "^2.1.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "buildstamp": "1.4.3"
  }
}
